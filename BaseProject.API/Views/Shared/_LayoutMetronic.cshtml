    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet" type="text/css" asp-append-version="true" />
    <!--end::Fonts-->
    <!--begin::Stylesheets-->
    <link href="~/Metronic/demo1/dist/assets/plugins/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/Metronic/demo1/dist/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/Metronic/demo1/dist/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/Metronic/demo1/dist/assets/css/style.bundle.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/Metronic/demo1/dist/assets/plugins/custom/cookiealert/cookiealert.bundle.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/css/Site.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css" asp-append-version="true" />

    <!--end::Stylesheets-->
    <!--begin::JQuery-->
    <script src="~/lib/jquery/jquery.min.js" asp-append-version="true"></script>

    <!--end::JQuery-->
    <!--begin::Javascript-->
    <script>var hostUrl = "/Metronic/demo1/dist/assets/";</script>


    <script src="~/Metronic/demo1/dist/assets/plugins/global/plugins.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/js/scripts.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/plugins/custom/datatables/datatables.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/js/widgets.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/plugins/custom/cookiealert/cookiealert.bundle.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/js/custom/widgets.js" asp-append-version="true"></script>
    <script src="~/Metronic/demo1/dist/assets/js/custom/apps/chat/chat.js" asp-append-version="true"></script>
    <script src="~/js/Site.js" asp-append-version="true"></script>
    <script src="~/js/DefaultDataTablesOptions.js" asp-append-version="true"></script>
    <script src="~/js/DefaultDateRangePickerOptions.js" asp-append-version="true"></script>
    <script src="~/js/DefaultDropzoneOptions.js" asp-append-version="true"></script>
    <script src="~/js/DefaultSelect2Options.js" asp-append-version="true"></script>
    <script src="~/lib/jquery.fileDownload/jquery.fileDownload.min.js" asp-append-version="true"></script>
    <script src="~/lib/highcharts/highcharts.min.js" asp-append-version="true"></script>
    <script src="~/lib/highcharts/modules/accessibility.min.js" asp-append-version="true"></script>
    <script src="~/lib/highcharts/modules/export-data.min.js" asp-append-version="true"></script>
