    <script src="~/js/custom/bundle.js" asp-append-version="true"></script>

    <!--end::Javascript-->

</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" data-kt-aside-minimize="on" class="header-fixed header-tablet-and-mobile-fixed toolbar-enabled toolbar-fixed aside-enabled aside-fixed" style="--kt-toolbar-height:55px;--kt-toolbar-height-tablet-and-mobile:55px">
    <!--begin::Main-->
    <!--begin::Root-->
    <div class="d-flex flex-column flex-root">
        <!--begin::Page-->
        <div class="page d-flex flex-row flex-column-fluid">

            <partial name="Metronic/_Aside" />

            <!--begin::Wrapper-->
            <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">

                <partial name="Metronic/_Header">

                    <!--begin::Content-->
                    <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                        @RenderBody()
                    </div>
                    <!--end::Content-->

                    <partial name="Metronic/_Footer">

            </div>
            <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::Root-->
    <!--end::Main-->
    <!--begin::Engage drawers-->
    <partial name="Metronic/_ChatDrawer">
        <partial name="Metronic/_HelpDrawer">
            <!--end::Engage drawers-->

            <partial name="Metronic/_EngageToolbar">

                <partial name="Metronic/_ScrollTop">

                    <partial name="Metronic/_CookieAlert">


</body>
<!--end::Body-->
</html>


